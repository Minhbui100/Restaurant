    bill_id integer PRIMARY KEY,
    cust_phone VARCHAR(15) REFERENCES customers(phone),  -- Added foreign key to customers
    location_name varchar(45) REFERENCES location(name),
    total NUMERIC(10, 2) DEFAULT 0,
