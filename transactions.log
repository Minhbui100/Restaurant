ON checked_dishes.name = orders_data.name;
    

UPDATE bill
SET total = (
    SELECT COALESCE(SUM(price * quantity), 0)
    FROM orders
    WHERE bill_id = $1
),
tax = total * 0.0625
WHERE bill_id = $1;
    
COMMIT

