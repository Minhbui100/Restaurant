  SELECT name, price, status
  FROM menu
  WHERE name IN ('Grilled Salmon', 'Chocolate Lava Cake')
),
checked_dishes AS (
  SELECT name, price
  FROM valid_dishes
  WHERE status = 'Available'
)
INSERT INTO orders (bill_id, name, price, quantity)
SELECT $1, checked_dishes.name, checked_dishes.price, orders_data.quantity
FROM checked_dishes
JOIN (VALUES ('Grilled Salmon', 3), ('Chocolate Lava Cake', 1)) AS orders_data(name, quantity)
ON checked_dishes.name = orders_data.name;
    

